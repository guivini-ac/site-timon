{
  "name": "@timon/config",
  "version": "1.0.0",
  "description": "Configurações compartilhadas",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@timon/types": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint.d.ts",
      "require": "./dist/eslint.js",
      "import": "./dist/eslint.js"
    }
  }
}